{
    "workbench.colorCustomizations": {
        "titleBar.activeBackground": "#00ABAC", // Top window bar
        "titleBar.inactiveBackground": "#00ABAC99", // Top window bar inactive (add '99' to end of hex)
        "titleBar.activeForeground": "#0c090a", // Top window bar - text

        "activityBarBadge.background": "#00ABAC", // Side bar - badge icon background
        "activityBarBadge.foreground": "#0c090a", // Side bar - badge text
        "activityBar.activeBorder": "#00ABAC" // Side bar - active indicator border
    },
    "cSpell.words": [
        "Delaplane",
        "Redmon",
        "Riddell"
    ],
    "folder-color.pathColors": [
        {
            "folderPath": "package-lock.json/",
            "color": "foldercolorizer.olive"
        },
        {
            "folderPath": "package.json/",
            "color": "foldercolorizer.olive"
        },
        {
            "folderPath": ".gitignore/",
            "color": "foldercolorizer.OrangeRed"
        },
        {
            "folderPath": "src/assets/js/",
            "color": "foldercolorizer.Gold"
        },
        {
            "folderPath": "src/assets/partials/",
            "color": "foldercolorizer.LightBlue"
        },
        {
            "folderPath": "src/assets/scss/",
            "color": "foldercolorizer.LightPink"
        }
    ]
}